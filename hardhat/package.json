  "bugs": {
    "url": "https://github.com/divyam96/zk-StealthGamble/issues"
  },
  "homepage": "https://github.com/divyam96/zk-StealthGamble.git#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.11.6",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.5.5",
    "chai": "^4.3.6",
    "circom_tester": "^0.0.11",
    "ethereum-waffle": "^3.4.4",
    "hardhat": "^2.9.7",
    "hardhat-contract-sizer": "^2.4.0",
    "hardhat-deploy": "^0.10.4",
    "hardhat-gas-reporter": "^1.0.7",
    "snarkjs": "^0.4.19",
    "web3-utils": "^1.7.0"
  },
  "dependencies": {
    "blake-hash": "^2.0.0",
    "circomlibjs": "^0.1.4",
    "circomlib": "^2.0.5",
    "ethers": "^5.6.8",
    "ffjavascript": "^0.2.52"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "isolatedModules": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "ts-jest",
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
